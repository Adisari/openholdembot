// 
WHEN NOT InBigBlind AND InTheMoney_SmallStack AND OpponentIsAllin
	// Facing small raise 0..2 bb
   WHEN AmountToCall < 2 AND listITMCat1 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat2 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat3 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat4 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat5 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat6 Call FORCE
   WHEN AmountToCall < 2 AND listITMCat7 Call FORCE
   // Facing raise 2..4 bb
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat1 Call FORCE
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat2 Call FORCE
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat3 Call FORCE
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat4 Call FORCE
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat5 Call FORCE
   WHEN AmountToCall >= 2 AND AmountToCall < 4 AND listITMCat6 Call FORCE
   // Facing raise 4..6 bb
   WHEN AmountToCall >= 4 AND AmountToCall < 6 AND listITMCat1 Call FORCE
