 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat3 RaiseMax Force
 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat4 RaiseMax Force
 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat5 RaiseMax Force
 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat6 RaiseMax Force
 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat7 RaiseMax Force
 When (user_Button OR user_SBlind) And user_Bubble_We_Are_Covered And Raises = 0 And Calls = 0 And listBubbleCat8 RaiseMax Force

 When user_BBlind And listBubbleCat1 RaiseMax Force
 When user_BBlind And listBubbleCat2 RaiseMax Force


//////////////////////////////////////////////////////////////////
//
// Middle Game - Process the All In Factors First
//////////////////////////////////////////////////////////////////
// Call all in within a multi-way pot. Add additional lines as required
//
When user_GameState_Middle And user_Opponent_ALL_IN And user_Aggression_Factor_1
 When user_BLA_None And listCat1 Call Force

//////////////////////////////////////////////////////////////////
// Middle Game Play - Blinds > Early blind values and Opponents = Full Ring
//////////////////////////////////////////////////////////////////
When user_GameState_Middle And user_Opponent_ALL_IN
 // If we entered the hand with a Raise, Call an all-in of 1 BB OR less
 When BotsLastAction = Raise And AmountToCall <= 1 Call Force
 // Get out of the multi-way all in
 When user_BLA_None And (CallsSinceLastRaise > 0 OR Raises > 1) Fold Force

When user_GameState_Middle And (user_SBlind OR user_BBlind)
 When user_Stack_FullRing_0_2 RaiseMax Force

When user_GameState_Middle And user_Stack_FullRing_12P 
 When BotsLastAction = Raise And listCat1 RaiseMax Force
 When BotsLastAction = Raise And listCat2 RaiseMax Force
 When BotsLastAction = Raise Fold Force

 When user_BLA_None And listCat1 RaiseBy 3 Force
 When user_BLA_None And listCat2 RaiseBy 3 Force
 When user_BLA_None And listCat3 RaiseBy 3 Force

 When user_BLA_None And listCat4 And Raises = 0 And Calls >= 0 RaiseBy 3 Force

 When user_BLA_None And listCat4 And Raises = 0 And Calls = 0 And user_EP RaiseBy 3 Force
 When user_BLA_None And listCat5 And Raises = 0 And Calls = 0 And user_EP RaiseBy 3 Force

 When user_BLA_None And listCat4 And Raises = 0 And Calls = 0 And (user_MP) RaiseBy 3 Force
 When user_BLA_None And listCat5 And Raises = 0 And Calls = 0 And (user_MP) RaiseBy 3 Force
 When user_BLA_None And listCat6 And Raises = 0 And Calls = 0 And (user_MP) RaiseBy 3 Force

 When user_BLA_None And listCat4 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseBy 3 Force
 When user_BLA_None And listCat5 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseBy 3 Force
 When user_BLA_None And listCat6 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseBy 3 Force
 When user_BLA_None And listCat7 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseBy 3 Force

When user_GameState_Middle And user_Stack_FullRing_5_12
 When listCat1 RaiseMax Force
 When listCat2 RaiseMax Force
 When listCat3 RaiseMax Force
 When listCat4 RaiseMax Force
 When listCat5 And Raises = 0 And Calls >= 0 RaiseMax Force

 When listCat5 And Raises = 0 And Calls = 0 And user_EP RaiseMax Force
 When listCat6 And Raises = 0 And Calls = 0 And user_EP RaiseMax Force

 When listCat5 And Raises = 0 And Calls = 0 And (user_MP) RaiseMax Force
 When listCat6 And Raises = 0 And Calls = 0 And (user_MP) RaiseMax Force
 When listCat7 And Raises = 0 And Calls = 0 And (user_MP) RaiseMax Force

 When listCat5 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseMax Force
 When listCat6 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseMax Force
 When listCat7 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseMax Force 
 When listCat8 And Raises = 0 And Calls = 0 And (user_Button OR user_SBlind) RaiseMax Force

When user_GameState_Middle And user_Stack_FullRing_0_5
 When listCat1 RaiseMax Force
 When listCat2 RaiseMax Force
 When listCat3 RaiseMax Force
 When listCat4 RaiseMax Force
 When listCat5 RaiseMax Force
 When listCat6 And Raises = 0 And Calls >= 0 RaiseMax Force

 When listCat7 And Raises = 0 And Calls = 0 RaiseMax Force

 When listCat8 And Raises = 0 And Calls = 0 And NOT (user_EP) RaiseMax Force


//////////////////////////////////////////////////////////////////
// Early Game Play
//////////////////////////////////////////////////////////////////
When user_GameState_Early And user_Opponent_ALL_IN And user_Aggression_Factor_1
 When user_BLA_None And listCat1 Call Force

When user_GameState_Early And user_Opponent_ALL_IN
 // If we entered the hand with a Raise, Call an all-in of 1 BB OR less
 When BotsLastAction = Raise And AmountToCall <= 1 Call Force
 // Get out of the multi-way all in
 When user_BLA_None And (CallsSinceLastRaise > 0 OR Raises > 1) Fold Force

When user_GameState_Early And (user_SBlind OR user_BBlind)
 When user_Stack_FullRing_0_2 RaiseMax Force

When user_GameState_Early And listCat1
 When BotsLastAction = Raise RaiseMax Force
 When BotsLastAction = None RaiseBy 3 Force

When user_GameState_Early And listCat2
 When BotsLastAction = Raise RaiseMax Force
 When BotsLastAction = None RaiseBy 3 Force

When user_GameState_Early And listCat3
 When BotsLastAction = Raise Fold Force
 When BotsLastAction = None And (user_Cutoff OR user_Button) RaiseBy 3 Force

When user_GameState_Early And listCat4
 When BotsLastAction = Raise Fold Force
 When BotsLastAction = None And (user_Cutoff OR user_Button) RaiseBy 3 Force



//////////////////////////////////////////////////////////////////
//  Aggression Factor 2 - Call in Small Blind when 2 or more callers
//
//  If you see that you bleed too many chips, tighten up the range
//  starting with the last line, and working your way up
//
//////////////////////////////////////////////////////////////////
When user_Aggression_Factor_2 And user_SBlind And Raises = 0 And Calls >= 2 And StackSize > 12
 When listCat3 Call Force
 When listCat4 Call Force
 When listCat5 Call Force
 When listCat6 Call Force
 When listCat7 Call Force
 When listCat8 Call Force
 // this statement will Call with ATC <Any Two Cards> @Trish change this
 When user_True Call Force

//////////////////////////////////////////////////////////////////
// Betting: user_RaiseQuad - Raise to 4 times the small 
// Raisers Raise. Sometimes they want a cheap flop
//////////////////////////////////////////////////////////////////
When user_RaiseQuad 
 When AmountToCall <= 1 RaiseBy 4 Force
 When AmountToCall <= 2 RaiseBy 8 Force
 When AmountToCall <= 3 RaiseBy 12 Force

When Others
 When user_BBlind And Raises = 0 SET user_BigBlind_We_Limped
 When Others Fold Force



